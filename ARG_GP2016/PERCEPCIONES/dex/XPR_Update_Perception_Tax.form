		EventScript	"Perception_Default_Tax Dummy Note Show Hide_CHG"		
		{
			Documentation	""
			Script	"call Check_For_Note,technicalname(window Perception_Default_Tax of form XPR_Update_Perception_Tax),'Dummy Note Show Hide';
if 'Dummy Note Show Hide'  = 1 then
	hide 'Note Absent Button - Toolbar';
	show 'Note Present - Toolbar';
else
	hide 'Note Present - Toolbar';
	show 'Note Absent Button - Toolbar';
end if;
"
			UserProperties	""
		}
		EventScript	"Perception_Default_Tax Note Absent Button - Toolbar_CHG"		
		{
			Documentation	""
			Script	"{--------------------------------------------------------------------------------
	Open the note for the window if possible.
--------------------------------------------------------------------------------}
local string l_Note_ID,l_Display_Name;
local boolean isthere;
local integer form_number;
local integer l_exists;

{-------------------------------------------------------------------------------
	Initialize local variables before calling global script
--------------------------------------------------------------------------------}
{get the current display name to display}
system 5017, window Perception_Default_Tax, l_Display_Name;

{get the original display name as the key into the notes file. This should be the
 same name as in the 'Dummy Record Show Hide' script.} 
set l_Note_ID to technicalname(window Perception_Default_Tax of form XPR_Update_Perception_Tax);

{check to see if someone created or deleted a note since entering the window}
call Check_For_Note,l_Note_ID,l_exists;
if 'Dummy Note Show Hide'=1 and l_exists=0 then
	call Warning_Dialog,1380,\"\",\"\",\"\";
elseif 'Dummy Note Show Hide'=0 and l_exists=1 then
	call Warning_Dialog,5500,\"\",\"\",\"\";
end if;

set isthere to false;
set form_number to 0;

{--------------------------------------------------------------------------------
	Check if a note form for this window is already open.  If one isn't, then
	get the number of an unused note form.  If all of the note forms are being
	used, then give a warning and abort the script.  Otherwise, open the note
	form for this window.
--------------------------------------------------------------------------------}
call Check_Note_ID_String,l_Note_ID,isthere,form_number;
if isthere=false then
	call Get_Next_Form_Note_To_Open,form_number;
	if form_number = 0 then
		abort script;
	end if;
end if;
if form_number = 1 then
	open form Form_Note_1 return to 'Dummy Note Show Hide';
	set 'Note ID' of window Form_Note_1 of form Form_Note_1 to l_Note_ID;
	set 'Display Name' of window Form_Note_1 of form Form_Note_1 to l_Display_Name;
	if not isthere then
		run script '(L) Dummy WIN PRE' of window Form_Note_1 of form Form_Note_1;
	end if;
elseif form_number = 2 then
	open form Form_Note_2 return to 'Dummy Note Show Hide';
	set 'Note ID' of window Form_Note_2 of form Form_Note_2 to l_Note_ID;
	set 'Display Name' of window Form_Note_2 of form Form_Note_2 to l_Display_Name;
	if not isthere then
		run script '(L) Dummy WIN PRE' of window Form_Note_2 of form Form_Note_2;
	end if;
elseif form_number = 3 then
	open form Form_Note_3 return to 'Dummy Note Show Hide';
	set 'Note ID' of window Form_Note_3 of form Form_Note_3 to l_Note_ID;
	set 'Display Name' of window Form_Note_3 of form Form_Note_3 to l_Display_Name;
	if not isthere then
		run script '(L) Dummy WIN PRE' of window Form_Note_3 of form Form_Note_3;
	end if;
elseif form_number = 4 then
	open form Form_Note_4 return to 'Dummy Note Show Hide';
	set 'Note ID' of window Form_Note_4 of form Form_Note_4 to l_Note_ID;
	set 'Display Name' of window Form_Note_4 of form Form_Note_4 to l_Display_Name;
	if not isthere then
		run script '(L) Dummy WIN PRE' of window Form_Note_4 of form Form_Note_4;
	end if;
else {form_number = 5}
	open form Form_Note_5 return to 'Dummy Note Show Hide';
	set 'Note ID' of window Form_Note_5 of form Form_Note_5 to l_Note_ID;
	set 'Display Name' of window Form_Note_5 of form Form_Note_5 to l_Display_Name;
	if not isthere then
		run script '(L) Dummy WIN PRE' of window Form_Note_5 of form Form_Note_5;
	end if;
end if;
"
			UserProperties	""
		}
