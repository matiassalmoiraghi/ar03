		EventScript	"Perception_Default_Tax_WIN_POST"		
		{
			Documentation	""
			Script	"local integer l_Ask;
if 'Handle Changes' = true then
	abort script;
end if;
if changed(window Perception_Default_Tax) then
	set l_Ask to ask(getmsg(1000),getmsg(2),getmsg(4),getmsg(1));
	if l_Ask = ASKBUTTON1 then
		set '(L) chkRecord' to 0;
		run script 'OK Button ';
		if '(L) chkRecord' = 1 then
		    abort close;
			abort script;
		end if;
		set 'Handle Changes' to true;
		clear changes window Perception_Default_Tax;
	elseif l_Ask = ASKBUTTON2 then
			clear changes window Perception_Default_Tax;
			set 'Handle Changes' to true;
		 	abort script;		
