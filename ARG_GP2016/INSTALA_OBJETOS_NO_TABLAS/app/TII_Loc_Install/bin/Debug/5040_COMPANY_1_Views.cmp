CREATE VIEW SD_SOP_GetCarrier_Info
AS
SELECT SD_Carr_SOP_Line.SOPNUMBE,
SD_Carr_SOP_Line.SOPTYPE,
SD_Carr_SOP_Line.CMPNTSEQ,
SD_Carr_SOP_Line.LNITMSEQ,
SD_Carr_SOP_Line.SDCRPRPCK,
SD_Carr_Carr.SDCRCDID,
SD_Carr_Carr.SDCRCDDESC,
SD_Carr_Pers.SDCRPRSNLID,
SD_Carr_Pers.SDCRPRSNLNAME,
SD_Carr_Pers.SDCRPRSNLNO,
SD_Carr_Pers.SDCRPRSNLPH,
SD_Carr_Vech.SDCRVHCLID,
SD_Carr_Vech.SDCRVHCLNAME,
SD_Carr_Vech.SDCRVHCLNO,
SD_Carr_Vech.SDCRVHCLREGNO_1,
SD_Carr_Vech.SDCRVHCLREGNO_2
FROM SD10206 AS SD_Carr_SOP_Line
INNER JOIN SD00107 AS SD_Carr_Carr ON SD_Carr_SOP_Line.SDCRCDID = SD_Carr_Carr.SDCRCDID
INNER JOIN SD00105 AS SD_Carr_Pers ON SD_Carr_SOP_Line.SDCRPRSNLID = SD_Carr_Pers.SDCRPRSNLID
INNER JOIN SD00106 AS SD_Carr_Vech ON SD_Carr_SOP_Line.SDCRVHCLID = SD_Carr_Vech.SDCRVHCLID

GO
