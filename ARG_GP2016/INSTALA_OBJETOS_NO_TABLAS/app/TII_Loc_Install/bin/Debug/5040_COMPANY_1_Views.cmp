CREATE VIEW SD_GetCarrier_Info
AS
SELECT	SD_HDR.ORDDOCID, SDCRPRPCK,  
SD_HDR.SDCRCDID, CRR.SDCRCDDESC,
SD_HDR.SDCRPRSNLID, PRS.SDCRPRSNLNAME, PRS.SDCRPRSNLNO, PRS.SDCRPRSNLPH,
VHL.SDCRVHCLID, VHL.SDCRVHCLNO, VHL.SDCRVHCLREGNO_1, VHL.SDCRVHCLREGNO_2
FROM SD10106 SD_HDR
INNER JOIN SD00107 CRR ON SD_HDR.SDCRCDID = CRR.SDCRCDID
INNER JOIN SD00105 PRS ON SD_HDR.SDCRPRSNLID = PRS.SDCRPRSNLID
INNER JOIN SD00106 VHL ON SD_HDR.SDCRVHCLID = VHL.SDCRVHCLID

GO
SET QUOTED_IDENTIFIER OFF 
GO
SET ANSI_NULLS ON 
GO

GRANT  SELECT ,  INSERT ,  DELETE ,  UPDATE  ON [dbo].[SD_GetCarrier_Info]  TO [DYNGRP] 
GO 

